<Alloy>
	<Collection src="works" />
	<Window id="window" class="container" layout="vertical">
		<Label text="Select a work" id="selectWorkLabel" />
		<ActivityIndicator id='loading' />
		<TableView dataCollection="works" id="workList">
			<TableViewRow _id="{_id}" title="{title}" attachments="{attachments}" leftImage="{cover_m}" onClick="uploadPhotoToWork"></TableViewRow>
		</TableView>
	</Window>
</Alloy>